{
  "name": "@zoralabs/v3",
  "version": "1.0.1",
  "private": false,
  "homepage": "https://github.com/ourzora/v3",
  "repository": {
    "type": "git",
    "url": "https://github.com/ourzora/v3.git"
  },
  "files": [
    "./dist/**/*",
    "./dist/*"
  ],
  "dependencies": {
    "@manifoldxyz/royalty-registry-solidity": "1.0.4",
    "@openzeppelin/contracts": "^4.3.2",
    "@rari-capital/solmate": "^5.9.0"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "@typechain/ethers-v5": "^8.0.5",
    "typechain": "^6.1.0",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{ts,js,css,md,sol}": "prettier --write",
    "*.sol": "solhint"
  },
  "scripts": {
    "build": "forge build && yarn typechain",
    "clean": "forge clean && rm -rf dist/typechain",
    "prepare": "husky install",
    "prepublishOnly": "rm -rf ./dist && forge clean && forge build && yarn typechain && cp -R contracts dist && cp -R addresses dist",
    "test": "forge test",
    "typechain": "typechain --target=ethers-v5 'dist/artifacts/*/*.json' --out-dir dist/typechain"
  }
}
